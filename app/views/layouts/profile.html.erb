<!DOCTYPE html>
<html>
  <head>
    <title>Profile</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'profile', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'bundle', 'data-turbolinks-track': 'reload' %>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
  </head>

  <body>
    <%= render partial: 'shared/flash_notice' %>
    <%= render partial: 'shared/flash_alert' %>
    <%= yield %>
  </body>
</html>
